{
  "name": "mudslide",
  "version": "0.10.4",
  "author": "robvanderleek@gmail.com",
  "description": "WhatsApp CLI",
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      ["@semantic-release/git", {
      "assets": ["package.json"],
      "message": "chore(release): ${nextRelease.version} [skip ci]"
      }]
    ]
  },
  "bin": {
    "mudslide": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && chmod a+x ./dist/index.js",
    "start": "yarn build && node ./dist/index.js",
    "dist:build": "ncc build dist/index.js -o dist-ncc",
    "dist:pkg": "pkg -t node16-macos-arm64 -o mudslide dist-ncc/index.js",
    "dist:bundle": "tar czf mudslide-macos-arm64.tgz mudslide"
  },
  "license": "ISC",
  "dependencies": {
    "@adiwajshing/baileys": "^4.4.0",
    "@hapi/boom": "^10.0.0",
    "commander": "^9.4.1",
    "pino": "^8.7.0",
    "qrcode-terminal": "^0.12.0",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/pino": "^7.0.5",
    "@types/signale": "^1.4.4",
    "@vercel/ncc": "^0.34.0",
    "pkg": "^5.8.0",
    "semantic-release": "^20.0.2",
    "typescript": "^4.9.3"
  }
}
